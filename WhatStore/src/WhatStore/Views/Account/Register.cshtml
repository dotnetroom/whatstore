@{
    Layout = null;
}
<html>
<head>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <title>Cadastro - WhatStore</title>
</head>

<body>
    <form id="formRegister" class="roow">
        <h1>Cadastro</h1>

        <label>Nome: <input type="text" /></label>
        <label>Sobrenome: <input type="text" /></label>
        <label>RG: <input type="text" /></label>
        <label>CPF: <input type="text" /></label>
        <label>Data de nascimento: <input type="datetime" name="date" /></label><br />
        <label>Genero: <input type="radio" name="genero" value="feminino" />Feminino <input type="radio" name="genero" value="masculino" />Masculino</label>
        <label>Email: <input type="text" /> </label>
        <label>Senha: <input type="password" /></label>
        <button type="submit">Registrar</button>

    </form>
</body>
</html>